<%= render partial: 'layouts/errors', locals: {object: @user}%>

<h2>Sign Up Page</h2>

<%= form_for(@user) do |f| %>
    <%= f.label :username %><br>
    <%= f.text_field :username %><br>

    <%= f.label :email %><br>
    <%= f.email_field :email %><br>

    <%= f.label :password %><br>
    <%= f.password_field :password %><br>

    <%= f.submit "Create New Account" %>
<% end %>